<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'
             xmlns:g='urn:import:com.google.gwt.user.datepicker.client'
             xmlns:res='urn:import:org.olim.client.views.CityStyleRecources'
>
    <ui:with type="com.travelSite.client.views.CityStyleRecources" field="res"/>
    <gwt:HTMLPanel>
        <div align="center">
            <gwt:HorizontalPanel>
                <gwt:SuggestBox ui:field="cityField"  />
                <g:DateBox ui:field="inDateField"/>
                <g:DateBox ui:field="outDateField"/>
            </gwt:HorizontalPanel>
        </div>
    </gwt:HTMLPanel>
</ui:UiBinder>